#%Module1.0#####################################################################
##
## BEAST modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\tBEAST - Adds BEAST to your path"
}

module-whatis	"BEAST-2.1.3"

set      iigb_modules    $::env(IIGB_MODULES)
set      modules_dir     $::env(MODULESHOME)

# for Tcl script use only
set	version		3.2.9
set	prefix		${modules_dir}/${version}
set	exec_prefix	${prefix}
set	datarootdir	${prefix}/share

prepend-path	PATH	         ${iigb_modules}/BEAST/2.1.3/bin
prepend-path	CLASSPATH        ${iigb_modules}/BEAST/2.1.3/lib

module		use		 ${modules_dir}/versions
