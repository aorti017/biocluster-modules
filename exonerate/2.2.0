#%Module1.0#####################################################################
##
## exonerate modulefile
##
proc ModulesHelp { } {
        global version prefix

        puts stderr "\t - Adds exonerate 2.2.0 to your path"
}

module-whatis   "exonerate-2.2.0"

set      iigb_modules    $::env(IIGB_MODULES)
set      modules_dir     $::env(MODULES_DIR)

# for Tcl script use only
set     version         3.2.9
set     prefix          ${modules_dir}/${version}
set     exec_prefix     ${prefix}
set     datarootdir     ${prefix}/share

prepend-path    PATH            ${iigb_modules}/exonerate/2.2.0/bin
prepend-path    MANPATH         ${iigb_modules}/exonerate/2.2.0/share/man

module          use             ${modules_dir}

